	 {
		"$schema": "node_modules/wrangler/config-schema.json",
		"name": "web-resources",
		"main": "src/index.js",
		"compatibility_date": "2025-03-03",
		"observability": {
			"enabled": true
		},
		"r2_buckets": [
			{
				"binding": "MY_BUCKET",  // Puedes cambiar "MY_BUCKET" a cualquier nombre que prefieras
				"bucket_name": "web-resources"  // Este es el nombre de tu bucket R2
			}
		]
	}
	
